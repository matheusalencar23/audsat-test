{
  "name": "audsat-test",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "clean": "rimraf ./dist",
    "init:db": "node init-db.js",
    "build:app": "ng build -c=production",
    "server:log": "wait-on ./db.json && ./node_modules/.bin/json-server --watch db.json --port 3000",
    "server:app": "wait-on ./dist/audsat-test/index.html && ./node_modules/.bin/http-server ./dist/audsat-test --port 3001 -o",
    "serve": "npm run init:db && run-p start server:log",
    "deploy": "npm run clean && npm run init:db && npm run build:app && run-p server:log server:app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.11",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.11",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "http-server": "^14.1.1",
    "json-server": "^0.17.4",
    "ngx-infinite-scroll": "^16.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular/cli": "~16.2.1",
    "@angular/compiler-cli": "^16.2.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "typescript": "~5.1.3",
    "wait-on": "^7.0.1"
  }
}
